{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test output",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/out/out.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "__pypackages__/3.10/lib",
            },
        },
        {
            "name": "Minimal",
            "type": "python",
            "request": "launch",
            "module": "pydantify",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "--keep-comments",
                "--canonical",
                "-i=${workspaceFolder}/examples/minimal/",
                "-V",
                "${workspaceFolder}/examples/minimal/interfaces.yang",
            ],
            "env": {
                "PYTHONPATH": "__pypackages__/3.10/lib",
            },
        },

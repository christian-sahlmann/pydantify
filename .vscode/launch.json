{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Make network automation pydantic",
            "type": "python",
            "request": "launch",
            "module": "yang2pydantic",
            "console": "integratedTerminal",
            "args": [
                "--format=pydantic",
                "--keep-comments",
                "--canonical",
                "--plugindir=${workspaceFolder}/yang2pydantic/plugins",
                "--path=${workspaceFolder}/examples/minimal/",
                "-V",
                "${workspaceFolder}/examples/minimal/interfaces.yang",
                "--output=./out.py"
            ],
            "env": {
                "PYTHONPATH": "__pypackages__/3.10/lib",
            },
        },
        {
            "name": "Debug output",
            "type": "python",
            "request": "launch",
            "program": "out.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "__pypackages__/3.10/lib",
            },
        }
    ]
}

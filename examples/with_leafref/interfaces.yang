module interfaces {
  namespace
  "http://ultraconfig.com.au/ns/yang/ultraconfig-interfaces";

  prefix if;

  description 'Example demonstarating leafref nodes';

  list interface {
    key "name";
    leaf test {
      type string;
    }
    leaf name {
      type string;
    }
  }
  leaf mgmt-interface {
    type leafref {
      path "../interface/name";
    }
  }
}

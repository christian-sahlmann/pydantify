module interfaces {
  namespace
  "http://ultraconfig.com.au/ns/yang/ultraconfig-interfaces";

  prefix if;

  description 'A simple module to validate descriptions and nodes,';

  leaf address {
    type string;
    default "test";
    description "Basic leaf node.";
  }

  leaf address2 {
    type string;
    default "test2";
    description "Basic leaf node.";
  }

  leaf port {
    type uint8;
    default 3;
    description "Basic int leaf node.";
  }
}
